alias r='qsubmitall'
alias rr='maxwell_clean;./qsubmit'
alias qs='qstat_dirs'
alias qsh='qstat $(<qid)'
alias er='less err'
alias cl='maxwell_clean'
alias o='cat outFileLog.1'

# git
